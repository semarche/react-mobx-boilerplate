{
  "name": "react-mobx-boilerplate",
  "version": "0.0.1",
  "description": "React, MobX, React-Router, MobX-Router.",
  "main": "webpack.config.js",
  "engines": {
    "node": ">=9.0",
    "npm": ">=5.0",
    "yarn": ">=1.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --mode development",
    "build": "rimraf ./dist && cross-env NODE_ENV=production webpack --mode production",
    "start": "cross-env NODE_ENV=production http-server ./dist -p 9999 --push-state -c-1"
  },
  "keywords": [
    "ES6",
    "ES7",
    "mobx",
    "mobx-router",
    "react",
    "react-router",
    "reactjs",
    "Webpack"
  ],
  "author": "Sergey Marchenko <sergey.dnepro@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-universal-import": "^3.0.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.2.0",
    "css-declaration-sorter": "^3.0.1",
    "css-loader": "^0.28.11",
    "css-mqpacker": "^6.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.3.1",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.9.0",
    "normalize.css": "^8.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-loader": "^2.1.5",
    "react-hot-loader": "4.3.3",
    "resolve-url-loader": "^2.3.0",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "sass-loader": "^7.0.3",
    "spa-http-server": "^0.9.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.7",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "mobx": "^5.0.2",
    "mobx-react": "^5.2.3",
